{
  "name": "repowatcher",
  "version": "1.0.1",
  "description": "Watch Github organization repositories for changes and push notification issues",
  "main": "/dist/server.js",
  "scripts": {
    "test": "jest",
    "grunt": "grunt",
    "start": "node ./dist/server",
    "prepare": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuarrdonOrg1/RepoWatcher.git"
  },
  "keywords": [
    "repository",
    "deleted",
    "created",
    "webhook",
    "push",
    "notification",
    "issues"
  ],
  "author": "Guarrdon",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "bugs": {
    "url": "https://github.com/GuarrdonOrg1/RepoWatcher/issues"
  },
  "homepage": "https://github.com/GuarrdonOrg1/RepoWatcher#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "grunt": "^1.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.22",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  }
}
