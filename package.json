{
  "name": "repowatcher",
  "version": "1.0.1",
  "description": "Watch Github organization repositories for changes and push notification issues",
  "main": "/dist/server.js",
  "scripts": {
    "test": "jest",
    "grunt": "grunt",
    "start": "node ./dist/server",
    "prepare": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuarrdonOrg1/RepoWatcher.git"
  },
  "keywords": [
    "repository",
    "deleted",
    "created",
    "webhook",
    "push",
    "notification",
    "issues"
  ],
  "author": "Guarrdon",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "bugs": {
    "url": "https://github.com/GuarrdonOrg1/RepoWatcher/issues"
  },
  "homepage": "https://github.com/GuarrdonOrg1/RepoWatcher#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jest": "^21.1.9",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^6.0.0-beta.17",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2"
  }
}
